{
    "name": "saft/saft-erfurt",
    "type": "library",
    "description": "Erfurt library integration. Erfurt is a mature library which shares a lot of the same objects Saft do. That package provides access to Erfurt using Saft.",
    "keywords": ["Saft", "Triple Store", "Erfurt", "RDF", "QueryCache"],
    "homepage": "https://safting.github.io/",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Natanael Arndt",
            "homepage": "http://aksw.org/NatanaelArndt",
            "email": "arndtn@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Konrad Abicht",
            "homepage": "http://www.inspirito.de",
            "email": "hi@inspirito.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "aksw/erfurt": ">=1.8",
        "saft/saft-rdf": ">=0.8",
        "saft/saft-store": ">=0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "saft/saft-test": ">=0.8"
    },
    "autoload": {
        "psr-4": {
            "Saft\\Addition\\Erfurt\\": "."
        }
    },
    "prefer-stable": true
}
